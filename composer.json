{
  "name": "phpro/soap-client",
  "description": "A general purpose SoapClient library",
  "keywords": ["soap"],
  "license": "MIT",
  "authors": [
    {
      "name": "Toon Verwerft",
      "email": "toon.verwerft@phpro.be"
    }
  ],
  "require": {
    "psr/log": "*",
    "doctrine/collections": "~1.3",
    "symfony/event-dispatcher": "~2.3",
    "symfony/console": "~2.3",
    "symfony/filesystem": "~2.3",
    "symfony/process": "~2.3"
  },
  "require-dev": {
    "phpro/grumphp": "~0.9",
    "squizlabs/php_codesniffer": "~2.3",
    "phpspec/phpspec": "~2.2",
    "phpunit/phpunit": "~4.7",
    "php-vcr/php-vcr": "~1.2",
    "php-vcr/phpunit-testlistener-vcr": "~1.1"
  },
  "suggest": {
    "doctrine/common": "For caching SOAP responses",
    "monolog/monolog": "For logging SOAP transactions"
  },
  "autoload": {
    "psr-0": {
      "Phpro\\SoapClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "PhproTest\\SoapClient\\": "test/"
    }
  },
  "bin": ["bin/soap-client"]
}
